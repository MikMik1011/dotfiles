alias ll='eza -la --no-permissions --octal-permissions --git --group-directories-first'
alias grep='rg'
alias cd..='cd ..'
alias ..='cd ..'
alias md='mkdir -px'
alias rmrf='rm -rf'
alias rr='rmrf'
alias g='git'
alias ga='git add'
alias ga.='git add .'
alias gra='git remote add origin'
alias gc='git commit -S -m'
alias gp='git push --set-upstream origin $(git rev-parse --abbrev-ref HEAD)'
alias gl='git log --oneline --decorate --graph'
alias gpl='git pull'
alias gd='git diff'
alias gs='git status'
alias cs='clear && pfetch'
alias cls='clear'
alias cds='cd ~ && cs'
alias pf='pfetch'
alias q='exit'
alias fi='fleek_install'
alias mysudo='sudo -E env "PATH=$PATH"'
