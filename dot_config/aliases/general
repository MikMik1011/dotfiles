alias ls='exa -l'
alias ll='exa -la'
alias cat='bat'
alias grep='rg'
alias cd..='cd ..'
alias ..='cd ..'
alias dbox='distrobox'
alias dboxc='distrobox create -i archlinux --init-hooks "pacman -Syu --noconfirm openssh git fish exa bat ripgrep micro" --name'
alias g='git'
alias ga='git add'
alias ga.='git add .'
alias gra='git remote add origin'
alias gc='git commit -S -m'
alias gp='git push --set-upstream origin $(git rev-parse --abbrev-ref HEAD)'
alias gl='git log --oneline --decorate --graph'
alias gpl='git pull'
alias gd='git diff'
alias gs='git status'
alias cs='clear && pfetch'
alias cls='clear'
alias pf='pfetch'
alias q='exit'
alias fi='fleek_install'
alias mysudo='sudo -E env "PATH=$PATH"'
alias mudo='mysudo'
alias protontricks='flatpak run com.github.Matoking.protontricks'
